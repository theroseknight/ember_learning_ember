<div class="col-xs-8 google-map-holder">
  {{log model.legs}}
  {{google-map latitude="37.8282" longitude="-98.5795" legs=model.legs action="googleMapsActions"}}
</div>
<div class="col-xs-2" id="right-landing-column">
  <table>
    <tbody>
      {{stats-displayer model=model distanceInMiles=model.distanceInMiles}}
    </tbody>
  </table>
</div>
