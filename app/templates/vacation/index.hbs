{{log "ember - app/templates/vacation/index.hbs"}}
<div class="row row-header">
  {{action-bar actionsArray=actionsArray transitionDestroy="/" id=model.id outerController=this idType="vacation"}}
</div>
<div class="row row-content">
  <div class="col-xs-2 leg-holder" id="left-landing-column">
    {{#each model.legs as |leg|}}
      {{partial "leg-container"}}
    {{/each}}
  </div>
  <div class="col-xs-8 google-map-holder">
    {{google-map latitude="37.8282" longitude="-98.5795" legModel=legModel}}
  </div>
  <div class="col-xs-2" id="right-landing-column">
  </div>
</div>
<div class="row row-footer">
  {{model-slider innerModel=innerModel action="focusedModel" modelId=model.id outerController=this idType="vacation"}}
</div>
